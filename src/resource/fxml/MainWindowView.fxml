<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import englishlearning.viewscontroller.*?>

<fx:root type="javafx.scene.layout.Pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
<GridPane prefHeight="400.0" prefWidth="600.0" fx:id="rootPane">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints maxHeight="127.0" minHeight="0.0" prefHeight="18.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="370.0" minHeight="10.0" prefHeight="359.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="50.0" minHeight="4.0" prefHeight="15.0" vgrow="SOMETIMES" />
  </rowConstraints>
<opaqueInsets>
<Insets />
</opaqueInsets>
<children>
    <GridPane>
        <columnConstraints>
            <ColumnConstraints hgrow="ALWAYS" />
            <ColumnConstraints hgrow="NEVER" />
        </columnConstraints>
        <children>
            <Pane>
                <Label text="English Learning"/>
            </Pane>
            <Pane GridPane.columnIndex="1">
                <Button fx:id="exitButton" mnemonicParsing="false" text="Exit" />
            </Pane>
        </children>
    </GridPane>
    <Pane GridPane.rowIndex="1" fx:id="contains"></Pane>
    <GridPane GridPane.rowIndex="2">
        <columnConstraints>
            <ColumnConstraints hgrow="ALWAYS" />
            <ColumnConstraints hgrow="NEVER" />
        </columnConstraints>
        <Pane>
            <Label fx:id="status1"/>
        </Pane>
        <Pane GridPane.rowIndex="1">
            <Label fx:id="status2"/>
        </Pane>
    </GridPane>
</children>
</GridPane>
</fx:root>